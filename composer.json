{
    "name": "pfwd/symfony-console-docker-template",
    "description": "Template for Symfony console within Docker",
    "type": "library",
    "authors": [
        {
            "name": "Peter Fisher"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "require": {
        "symfony/console": "~4.0",
        "php": ">=7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.3",
        "codeception/codeception": "^2.5"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-bin-dir": "bin"
    }
}
